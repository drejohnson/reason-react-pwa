{
  "name": "reason-react-pwa",
  "version": "0.1.0",
  "scripts": {
    "start": "serve -s dist",
    "dev": "concurrently \"bsb -make-world -w\" \"webpack-serve ./webpack.config.js\"",
    "prebuild": "npm run clean:dist",
    "build": "bsb -make-world && NODE_ENV=production webpack",
    "postbuild": "npm run copy:public",
    "copy:public": "node tools/copy.js",
    "clean:dist": "node tools/clean.js",
    "critical": "node tools/critical.js"
  },
  "keywords": [
    "BuckleScript",
    "ReactReact",
    "Material Components"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^8.6.5",
    "postcss-preset-env": "^5.2.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.4.0",
    "reason-loadable": "^0.4.0",
    "reason-react": "^0.4.2",
    "reason-reroute": "^0.0.4",
    "rmwc": "^1.7.6"
  },
  "devDependencies": {
    "bs-dynamic-import": "^0.3.0",
    "bs-platform": "^4.0.0",
    "concurrently": "^3.6.0",
    "connect-history-api-fallback": "^1.5.0",
    "critical": "^1.3.3",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "koa-connect": "^2.0.1",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-loader": "^2.1.6",
    "postcss-scss": "^1.0.6",
    "sass-loader": "^7.0.3",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.4",
    "webpack-serve": "^2.0.2",
    "webpackbar": "^2.6.1",
    "workbox-webpack-plugin": "^3.3.1"
  }
